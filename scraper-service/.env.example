# Database Configuration
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/exerciser"

# ClubReady API Configuration
# Used by: Club Pilates, CycleBar, Row House, Pure Barre, YogaSix
# To obtain: Email support@clubready.com to request API access
CLUBREADY_API_KEY=your_clubready_api_key_here

# ClubReady Store IDs for each brand/location
# Format: Brand-specific store ID from ClubReady
CLUBREADY_STORE_ID_CLUB_PILATES=your_store_id
CLUBREADY_STORE_ID_CYCLEBAR=your_store_id
CLUBREADY_STORE_ID_ROW_HOUSE=your_store_id
CLUBREADY_STORE_ID_PURE_BARRE=your_store_id
CLUBREADY_STORE_ID_YOGASIX=your_store_id

# MINDBODY API Configuration
# Used by: Independent studios, potentially Title Boxing, barre3, StretchLab
# To obtain: Apply at https://developers.mindbodyonline.com/
MINDBODY_API_KEY=your_mindbody_api_key_here

# MINDBODY Site IDs for each studio
# Format: Numeric site ID from MINDBODY
MINDBODY_SITE_ID_TITLE_BOXING=your_site_id
MINDBODY_SITE_ID_BARRE3=your_site_id
MINDBODY_SITE_ID_STRETCH_LAB=your_site_id

# Optional: MINDBODY Staff Credentials (for enhanced access)
MINDBODY_USERNAME=your_staff_username
MINDBODY_PASSWORD=your_staff_password

# Custom Platform APIs
# These need to be discovered individually
F45_API_KEY=your_f45_api_key
SPENGA_API_KEY=your_spenga_api_key
RUMBLE_API_KEY=your_rumble_api_key
SOLIDCORE_API_KEY=your_solidcore_api_key

# Vagaro API (for Jia Ren Yoga)
# To obtain: Contact Vagaro support
VAGARO_API_KEY=your_vagaro_api_key
VAGARO_MERCHANT_ID=your_merchant_id

# Rate Limiting
# Requests per minute for each API
CLUBREADY_RATE_LIMIT=60
MINDBODY_RATE_LIMIT=60

# Logging
LOG_LEVEL=info
